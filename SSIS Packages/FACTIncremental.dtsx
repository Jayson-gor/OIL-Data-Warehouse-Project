          <DTS:Variables />
          <DTS:ObjectData>
            <pipeline
              version="1">
              <components>
                <component
                  refId="Package\Incremental load for Fact Finance\Data Flow Task\Join for Finance"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                  description="OLE DB Source"
                  name="Join for Finance"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    d.date_key AS DateKey,
    c.company_key AS CompanyKey,
    a.AccountKey AS AccountKey,
    cr.CurrencyKey AS CurrencyKey,
    p.ProductKey AS ProductKey,
    j.JournalKey AS JournalKey,
    j.Name AS JournalName,
    a.AccountType AS AccountType,
    a.CurrencyID AS CurrencyID,
    a.Debit,
    a.Credit,
    a.Balance,
    a.AmountCurrency,
    a.AmountTotal,
    a.State,
    TRY_PARSE(a.Date_Move AS DATE USING 'en-US') AS Date_Move,  -- Ensures conversion
    cr.LatestRate,
    cr.LatestRateDate
FROM 
    Accounts_Dim a
JOIN 
    date_dim d ON TRY_PARSE(a.Date_Move AS DATE USING 'en-US') = d.full_date
JOIN 
    Company_dim c ON a.CompanyID = c.id
JOIN 
    Currency_Dim cr ON a.CurrencyID = cr.CurrencyID
JOIN 
    Journal_Dim j ON a.JournalID = j.JournalID
LEFT JOIN 
    Product_Dim p ON a.ProductID = p.ProductID
WHERE 
    a.State = 'posted'
    AND p.ProductKey IS NOT NULL
    AND j.Name = 'Customer Invoices'
