                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
    -- DateKey from date_dim
    d.date_key AS DateKey,

    -- SalesKey from dim_Sales
    s.SalesKey AS SalesKey,

    -- CompanyKey from company_dim
    c.company_key AS CompanyKey,

    -- ProductKey from product_dim
    p.ProductKey AS ProductKey,

    -- LocationKey from location_dim
    l.LocationKey AS LocationKey,

    -- Collection Metrics from dim_Sales
    s.CurrencyRate,
    s.DateOrder,
    s.WarehouseID,
    s.DeliveryStatus,
    s.Origin,
    s.State,
    s.Discount,
    s.PriceTotal,
    s.QtyInvoiced,
    s.QtyDelivered,
    s.ProductUOM,
    s.PriceUnit,
    s.QtyObserved,
    s.QtyAt20
FROM
    dim_Sales s

-- Join with date_dim on DateOrder
JOIN 
    date_dim d ON CONVERT(DATE, s.DateOrder) = d.full_date

-- Join with company_dim on CompanyID
JOIN 
    Company_dim c ON s.CompanyID = c.id

-- Join with product_dim on ProductID
JOIN 
    Product_Dim p ON s.ProductID = p.ProductID

-- Join with location_dim on WarehouseID
JOIN 
    Location_Dim l ON s.WarehouseID = l.WarehouseID

-- Filter to only include deliveries with 'full' or 'partial' status
WHERE 
    s.DeliveryStatus IN ('full', 'partial');</property>
